{
  "name": "@instaclone/frontend",
  "version": "0.1.0",
  "description": "Instagram clone on react and styled components",
  "private": true,
  "scripts": {
    "dev:storybook": "start-storybook -p 3006",
    "dev:server": "micro ./server",
    "dev:webpack": "webpack-dev-server",
    "dev": "concurrently -n 'server,webpack,storybook' -k 'npm run dev:server' 'npm run dev:webpack' 'npm run dev:storybook'",
    "start": "NODE_ENV=production micro ./server",
    "clean": "rimraf dist/*",
    "build:prod": "cross-env NODE_ENV=production run-s clean build",
    "build:storybook": "build-storybook",
    "build": "webpack",
    "test:lint": "eslint .",
    "test": "npm run test:lint",
    "prepush": "npm test",
    "precommit": "npm run test:lint",
    "commitmsg": "cat .git/COMMIT_EDITMSG | commitlint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergeysova/InstaClone.git"
  },
  "keywords": [
    "react",
    "webpack",
    "styled",
    "components",
    "atomic",
    "design",
    "redux"
  ],
  "author": "Sergey Sova <mail@sergeysova.com> (https://sergeysova.com)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/sergeysova/InstaClone/issues"
  },
  "homepage": "https://github.com/sergeysova/InstaClone#readme",
  "dependencies": {
    "micro": "^9.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-symbiote": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.1.6",
    "styled-normalize": "^4.0.0"
  },
  "devDependencies": {
    "@atomix/eslint-config": "^6.3.0",
    "@atomix/eslint-config-react": "^6.3.0",
    "@commitlint/cli": "^6.1.3",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-backgrounds": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/addon-storysource": "^3.4.0-alpha.8",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "ajv": "^6.2.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-name": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.4",
    "cross-env": "^5.1.4",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.18.2",
    "glob": "^7.1.2",
    "happypack": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2",
    "react-svg-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "storybook-addon-jsx": "^5.3.0",
    "typescript": "^2.8.1",
    "typescript-styled-plugin": "^0.6.0",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2",
    "whatwg-fetch": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
